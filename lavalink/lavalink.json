{
    "Image": "ghcr.io/ma4z-sys/yolks:java",
    "Cmd": "",
    "Env": [
      "START=java -jar Lavalink.jar"
    ],
    "StopCommand": "^^C",
    "features": [],
    "Scripts": {
      "Install": [
        {
          "Uri": "https://github.com/lavalink-devs/Lavalink/releases/latest/download/Lavalink.jar",
          "Path": "Lavalink.jar"
        },
        {
          "Uri": "https://raw.githubusercontent.com/lavalink-devs/Lavalink/master/LavalinkServer/application.yml.example",
          "Path": "application.yml"
        }
      ]
    },
    "Variables": {
      "SERVER_PORT": {
        "type": "text",
        "default": "2333",
        "required": true
      }
    },
    "Name": "Lavalink",
    "Description": "Lavalink v4 - High-performance audio sending node",
    "Author": "otakuzintv@gmail.com",
    "AuthorName": "ShindoZk",
    "Meta": {
      "Version": "1.0"
    }
  }
  
